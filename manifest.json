

{
  "manifest_version": 3,
  "name": "Olamovies Shortner bypass",
  "description": "Bypass Olamovies shortner links",
  "version": "1.1.0",
  "icons": {
    "16": "image.png",
    "48": "image.png",
    "128": "image.png"
  },
  "action": {
    "default_icon": {
      "16": "image.png",
      "48": "image.png",
      "128": "image.png"
    }
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules_1.json"
      }
    ]
  }
}

